{
  "name": "moonlock-cli",
  "version": "1.0.0",
  "description": "CLI tool for Moonlock Dashboard - Claude Code optimization",
  "main": "dist/cli.js",
  "bin": {
    "moonlock": "./dist/cli.js"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "dev": "ts-node src/cli.ts",
    "package": "./scripts/package.sh",
    "test": "./scripts/test.sh",
    "clean": "rm -rf dist packages && mkdir -p dist",
    "tsc": "tsc",
    "validate-package": "npm pack --dry-run",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "claude",
    "optimization",
    "cli",
    "moonlock",
    "token-tracking"
  ],
  "author": "Moonlock Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
