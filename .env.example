# Netlify Function Environment Variables
# Copy to .env for local development

# API Security
NETLIFY_API_SECRET=your-secure-api-secret-here
API_SECRET=your-secure-api-secret-here

# Database Configuration (optional - uses memory cache if not provided)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# Sync Configuration
SYNC_INTERVAL=30
NETLIFY_FUNCTION_URL=https://your-site.netlify.app/.netlify/functions/session-sync

# Development Settings
NODE_ENV=development
DEBUG=true